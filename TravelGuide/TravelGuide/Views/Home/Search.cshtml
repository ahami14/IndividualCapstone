
@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>
<h3>@ViewBag.Message</h3>


<!DOCTYPE html>
<html>
<head>
    <style>
        #map {
            width: 100%;
            height: 400px;
            background-color: grey;
        }
    </style>
</head>
<body>
    <h3></h3>

    <div id="map"></div>
    <script>

        function initMap() {

            var uluru = { lat: 43.0389, lng: -87.9065 };

            var map = new google.maps.Map(
                document.getElementById('map'), { zoom: 4, center: uluru });

            var marker = new google.maps.Marker({ position: uluru, map: map });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=@APIkeys.googkey&callback=initMap&libraries=places">
    </script>
</body>
</html>
